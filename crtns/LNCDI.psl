	EXAMPLE:
		$$AMOMDT^LNCDI([LN]CID,[LN]TRM,[LN]PTRM,[LN]DFP,[LN]DIST1FRE)
	*/

	new NJD,MDT,ODT

	if '$L(TRM) set TRM=PTRM
	set FRE="-"_FRE
	set NJD=$$NJD^UFRE(JD,FRE) if ER quit "" // 09/30/99 - SIVCHUKA
	set ODT=NJD do ^UMDT set AMOMDT=MDT

	quit AMOMDT


